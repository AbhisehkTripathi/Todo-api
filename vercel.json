{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb://localhost:27017/crudDB",
      "OTHER_ENV_VARIABLE": "value"
    }
  }
  